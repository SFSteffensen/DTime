{
  "productName": "DTime",
  "version": "1.0.0",
  "identifier": "sfsteffensen.dev",
  "build": {
    "beforeDevCommand": "yarn run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "yarn run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "DTime",
        "width": 800,
        "height": 600
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
